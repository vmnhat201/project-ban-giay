        <servlet-name>ManagerControl</servlet-name>
        <url-pattern>/manager</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteManagerControl</servlet-name>
        <url-pattern>/delete-manager</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddProductControl</servlet-name>
        <url-pattern>/add-product</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HistorySaleControl</servlet-name>
        <url-pattern>/history-sale</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
